<%-include("partial/header")-%>
  <div class="row">
  <% arr.forEach(function(element){ %>
    <div class="col-lg-3 col-md-6 col-sm-9 box">
      <div class="card">
        <div class="card-header">
          <h3><%= element.product %></h3>
        </div>
        <div class="card-body">
          <img src="<%=element.img%>" alt="Product Image" class="image">
          <span>Rs.</span> <span><%=element.price%></span>
            <button class="btn btn-lg btn-block btn-outline-primary" onclick="location.href = '/buy/electronics/<%=element.id%>';">See Details</button>
          <!-- </form> -->
        </div>
      </div>
    </div>
  <% }) %>
  </div>
<%-include("partial/footer")-%>
